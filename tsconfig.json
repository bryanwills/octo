{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "esModuleInterop": true,
    "lib": ["esnext", "dom"],
    "paths": {
      "/lib/*": ["./lib/*"],
      "/src/*": ["./src/*"],
      "/types/*": ["./types/*"]
    },
    "sourceMap": true,
    "strict": true,
    "target": "esnext",
    "useDefineForClassFields": true
  }
}
