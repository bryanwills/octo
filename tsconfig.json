{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "esModuleInterop": true,
    "lib": ["ESNext", "DOM"],
    "module": "ESNext",
    "moduleResolution": "NodeNext",
    "paths": {
      "/*.ts": [
        "./*.ts"
      ],
      "/*.js": [
        "./*.js"
      ],
      "/*": [
        "./*.ts",
        "./*.js",
        "./*"
      ]
    },
    "sourceMap": true,
    "strict": true,
    "target": "ESNext",
    "useDefineForClassFields": true
  }
}
